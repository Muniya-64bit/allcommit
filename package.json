{
  "name": "allcommit",
  "displayName": "AllCommit",
  "description": "This a vscode extension that  generate auto commit descriptions and give code reviews and let user to add comments to  a file easily",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "allcommit.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "allcommit.saveCommit",
        "title": "Save Commit"
      },
      {
        "command": "allcommit.push",
        "title": "Push"
      },
      {
        "command": "allcommit.addComment",
        "title": "Add Comments"
      },
      {
        "command": "allcommit.selectBranch",
        "title": "Select Branch"
      },
      {
        "command": "allcommit.changeDescriptionLanguage",
        "title": "Select Your Language"
      },
      {
        "command": "allcommit.codereview",
        "title": "Review Code"
      },{
        "command": "allcommit.pull",
        "title": "Pull"
      }
      
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "uuid": "^11.0.3"
  }
}
